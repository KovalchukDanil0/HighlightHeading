{
  "manifest_version": 3,
  "name": "Highlight Heading",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "permissions": ["storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "HighlightHeadings@gomofob.com",
      "strict_min_version": "109.0"
    }
  }
}
